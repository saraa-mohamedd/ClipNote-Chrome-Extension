{
    "manifest_version": 3,
    "version": "1.0",
    "name": "ClipNote",
    "action": {
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "description": "Organize and keep track of online links and notes with this handy-dandy extension.",
    "permissions": [
        "tabs",
        "fileSystem",
        "file:///*"
    ],
    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "48": "logo48.png",
        "128": "logo128.png",
        "256": "logo.png",
        "512": "logo.png",
        "1024": "logo.png",
        "2048": "logo.png"
    }
}