{
    "manifest_version": 3,
    "version": "1.0",
    "name": "ClipNote",
    "action": {
        "default_popup": "index.html",
        "default_icon": "logos/logo.png"
    },
    "description": "Organize and keep track of online links and notes with this handy-dandy extension~",
    "permissions": [
        "tabs",
        "fileSystem",
        "file:///*"
    ],
    "icons": {
        "16": "logos/logo16.png",
        "32": "logos/logo32.png",
        "48": "logos/logo48.png",
        "128": "logos/logo128.png",
        "256": "logos/logo.png",
        "512": "logos/logo.png",
        "1024": "logos/logo.png",
        "2048": "logos/logo.png"
    }
}